@{  
    if (ViewData["Type"].ToString() == "NewUser")
    {
        <div class="col-xs-6">
            <label class="col-xs-2" >New User</label>
             <select  title="User Type" class="col-xs-3" data-ng-model="dtUser.newuser.typeid"
                data-ng-show="!isNewVisible(dtUserType)"
                data-ng-options="tp.id as tp.title for tp in getList(dtUserType)">
            </select> 
            @{
                ViewData["Type"] = "NewTypeMenu";
                @Html.Partial("UserWindow")
                ViewData["Type"] = "NewType";
                @Html.Partial("UserWindow")
                ViewData["Type"] = "";
            }
            <div data-ng-show="!isNewVisible(dtUserType)" class="col-xs-6" >
                <input type="text" title="User Name" class="col-xs-8" data-ng-model="dtUser.newuser.title"/> 
                <input type="text" title="Description" class="col-xs-12" data-ng-model="dtUser.newuser.attrib.attrib.desc"/>
                <input type="text" title="Attribute" class="col-xs-4" data-ng-model="dtUser.newuser.attrib.attrib"/>
                <div class="btn-sm btn-info col-xs-4"
                     data-ng-click="dtUser.saveObj()">Add
                </div>
                <div class="btn-sm btn-warning col-xs-4"
                    data-ng-click="setNewVisible(dtUser)">Close
                </div>
            </div>
        </div>
    }    
    if( ViewData["Type"].ToString() == "NewTypeMenu" )
    {
        @* new role entry*@
        <div data-ng-show="!dtUserType.showNew()"
            data-ng-click="dtUserType.setNew()" 
                class="btn-sm btn-info col-xs-2">Add Type
        </div>        
    }
    if (ViewData["Type"].ToString() == "NewType")
    {
        <div class="col-xs-6"  data-ng-show="dtUserType.showNew()">
            <label class="col-xs-4" >Type</label>
            <input type="text" class="col-xs-8" data-ng-model="dtUserType.newtype.title"/>
            <input type="text" title="Description" class="col-xs-12" data-ng-model="dtUserType.newtype.attrib.attrib.desc"/>
            <input type="text" title="Attribute" class="col-xs-4" data-ng-model="dtUserType.newtype.attrib.attrib"/>
            <div class="btn-sm btn-info col-xs-4"
                data-ng-click="dtUserType.saveNew()">Add
            </div>
            <div class="btn-sm btn-warning col-xs-4"
                data-ng-click="dtUserType.hideNew()">Close
            </div>
        </div>
    }

    if( ViewData["Type"].ToString() == "UserMenu" )
    {
         @* User*@    
        <div class="panel col-xs-2">
        <a class="col-xs-12" href="~/View/{{user.title}}" >
            <div class="btn-default col-xs-12">{{user.usertype}}</div>
            <div class="btn-info col-xs-12">{{user.title}}</div></a>
        </div>
    }
    if( ViewData["Type"].ToString() == "User" )
    {
         @* User*@    
        <div class="panel col-xs-2">
        <div class="col-xs-12" data-ng-click="setVis" >
            <div class="btn-default col-xs-12">{{user.type}}</div>
            <div class="btn-info col-xs-12">{{user.title}}</div></div>
        </div>
    }
}